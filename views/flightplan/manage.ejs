<html>
<head>
<title>Golden Eagle Fligh Plan</title>

<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">

<!-- Optional theme -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap-theme.min.css">


</head>

<body style="padding-top: 50px;">


<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <a class="navbar-brand" href="#">Golden Eagle Flight Plan</a>
    </div>
    <div id="navbar" class="collapse navbar-collapse">
      <ul class="nav navbar-nav">
      
       <% if(req.session.userType === "administrator"){%>
	        <li class="dropdown">
	          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Stages <span class="caret"></span></a>
	          <ul class="dropdown-menu" role="menu">
	            <li><a href="#">Manage Stages</a></li>
	          </ul>
	        </li>
	        <li class="dropdown">
	          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Runways <span class="caret"></span></a>
	          <ul class="dropdown-menu" role="menu">
	            <li><a href="#">Manage Runways</a></li>
	          </ul>
	        </li>
	    <% } %>
	        
	     
		<li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Flightplans <span class="caret"></span></a>
          <ul class="dropdown-menu" role="menu">
           
           <% if(req.session.userType === "administrator"){%>
           		<li><a href="#">Manage Flightplans</a></li>
           <% } %>
   
            <% if(req.session.userType === "student"){ %>
            	<li><a href="#">My Flightplans</a></li>
            <% } %>
            
            <% if(req.session.userType === "advisor"){%>
            	<li><a href="#">All User Flightplans</a></li>
            <% } %>
            
          </ul>
        </li>
      
      
        <li><a href="/logout">Logout</a></li>
      </ul>
    </div><!--/.nav-collapse -->
  </div>
</nav>
    
<div style="padding: 30px 30px;">
<h3>Manage Department Flightplans</h3>
	<table class="table table-striped table-bordered">
	<thead>
		<tr><td>Name</td><td>Options</td></tr>
	</thead>
	<tbody>
		<% for(var i=0; i<flightplans.length; i++) { %>
		
			<tr><td width="80%"><%= flightplans[i].name %></td>
				<td>
					<a href="/flightplan/view?id=<%=flightplans[i].id%>" class="btn btn-default">View</a> 
				</td></tr>
		
		<% } %> 
	</tbody> 
	</table>
</div>
  
  
  
<!-- Latest compiled and minified JavaScript -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
</body>
</html>