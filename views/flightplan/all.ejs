<%- partial('../header') %>

<div style="padding: 30px 30px;">
<h3>Manage Department Flightplans</h3>
	<table class="table table-striped table-bordered">
	<thead>
		<tr><td>Name</td><td>Options</td></tr>
	</thead>
	<tbody>
		<% for(var i=0; i<flightplans.length; i++) { %>

			<tr><td width="80%"><%= flightplans[i].user.username %>'s Flightplan</td>
				<td>
					<a href="/user/flightplan?fid=<%=flightplans[i].user.flightplan%>&oid=<%=flightplans[i].user.id%>" class="btn btn-default">View</a>
				</td></tr>

		<% } %>
	</tbody>
	</table>
</div>


<!-- Latest compiled and minified JavaScript -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>

<%- partial('../footer') %>
